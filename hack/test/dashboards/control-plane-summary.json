              "sources": [
                {
                  "name": "uptime",
                  "query": "join(\n  ts(\"kubernetes.node.uptime\", cluster=\"${cluster_name}\" and node_role=\"control-plane\" ) AS ts1 \n  INNER JOIN \n  ts(\"kubernetes.node.info\", cluster=\"${cluster_name}\" and node_role=\"control-plane\") AS ts2\n  USING(nodename), metric='nodedetails', source=ts1.source, cluster=ts1.cluster, nodename=ts1.nodename, node_role=ts2.node_role, internal_ip=ts2.internal_ip, provider_id=ts2.provider_id, kubelet_version=ts2.kubelet_version, os_image=ts2.os_image,\n  ts1 / 60000\n)",
                  "disabled": true,
                  "secondaryAxis": false,
                  "queryType": "WQL",
                  "querybuilderEnabled": false,
                  "sourceDescription": ""
                },
                {
                  "name": "nodeCondition",
                  "query": "join(\n  ts(\"kubernetes.node.status.condition\"  and condition=\"Ready\" AND nodename=\"${node_name}\" AND node_role=\"control-plane\" AND  cluster=\"${cluster_name}\") AS ts1 \n  INNER JOIN \n  ${uptime} AS ts2\n  USING(nodename), metric='nodedetails', source=ts1.source, cluster=ts1.cluster, nodename=ts1.nodename, node_role=ts2.node_role, internal_ip=ts2.internal_ip, provider_id=ts2.provider_id, kubelet_version=ts2.kubelet_version, os_image=ts2.os_image, ready=ts1.status, uptime=ts2.value, \n  ts2\n  )",
                  "disabled": false,
                  "secondaryAxis": false,
                  "queryType": "WQL",
                  "querybuilderEnabled": false,
                  "sourceDescription": ""
                }
              ],
              "noDefaultEvents": true,
              "description": "status=true means node is in Ready state.",
              "anomalyType": "both"
            }
          ],
          "heightFactor": 80
        },
